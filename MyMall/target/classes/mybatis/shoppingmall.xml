<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="shoppingmall">
	<select id="list" resultType="Shoppingmall">
		select * from mymall.shoppingmall;
	</select>
	<insert id="add">
		insert into mymall.shoppingmall (name,content,url,best_code,admin_id)
		 values (#{name},#{content},#{url},#{bestCode},#{adminId})
	</insert>
</mapper>